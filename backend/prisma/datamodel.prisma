type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  lists: [List!]
  events: [Event!]
}

type List {
  id: ID! @id
  title: String!
  places: [Place!] @relation(name: "PlacesOfList", onDelete: CASCADE)
  user: User!
}

type Place {
  id: ID! @id
  gcmsId: String
  name: String
  list: List! @relation(name: "PlacesOfList")
}

type Event {
  id: ID! @id
  gcmsId: String! @unique
  user: User!
}
