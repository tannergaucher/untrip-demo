type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  places: [Place!]
  events: [Event!]
}

type Place {
  id: ID! @id
  gcmsId: String! @unique
}

type Event {
  id: ID! @id
  gcmsId: String! @unique
}

type List {
  id: ID! @id
  name: String!
  places: [Place!]!
}
