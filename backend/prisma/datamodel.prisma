type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  lists: [List!]
  events: [Event!]
}

type Place {
  id: ID! @id
  gcmsId: String!
  list: List!
}

type Event {
  id: ID! @id
  gcmsId: String! @unique
  user: User!
}

type List {
  id: ID! @id
  title: String!
  places: [Place!]
  user: User!
}
